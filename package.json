{
  "name": "react-snackbar-alert",
  "version": "1.8.0",
  "description": "Simple snackbar-style notifications for React",
  "main": "build/index.js",
  "files": [
    "build/index.js",
    "build/Snackbar.js",
    "build/SnackbarManager.js",
    "styles"
  ],
  "scripts": {
    "prepublishOnly": "npm run build",
    "build": "babel src -d build",
    "build:watch": "babel src -d build --watch",
    "clean": "rm -rf build",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint src",
    "prettify": "prettier --write 'src/*.js' 'examples/src/**/*.js'"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/joeattardi/react-snackbar-alert.git"
  },
  "homepage": "https://joeattardi.github.io/react-snackbar-alert/",
  "keywords": [
    "react",
    "snackbar",
    "alert",
    "notification"
  ],
  "author": "Joe Attardi <jattardi@gmail.com> (https://joeattardi.codes)",
  "license": "MIT",
  "devDependencies": {
    "@babel/cli": "^7.4.4",
    "@babel/core": "^7.4.5",
    "@babel/preset-env": "^7.4.5",
    "@babel/preset-react": "^7.0.0",
    "babel-jest": "^24.8.0",
    "enzyme": "^3.9.0",
    "enzyme-adapter-react-16": "^1.13.2",
    "eslint": "^5.16.0",
    "eslint-config-prettier": "^4.3.0",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-react": "^7.13.0",
    "husky": "^2.3.0",
    "jest": "^24.8.0",
    "lint-staged": "^8.1.7",
    "prettier": "1.17.1",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "styled-components": "^4.2.0"
  },
  "peerDependencies": {
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "styled-components": "^4.2.0"
  },
  "dependencies": {
    "prop-types": "^15.7.2",
    "react-transition-group": "^4.0.1",
    "uuid": "^3.3.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/*.js": [
      "prettier --write",
      "git add"
    ],
    "examples/src/**/*.js": [
      "prettier --write",
      "git add"
    ]
  }
}
